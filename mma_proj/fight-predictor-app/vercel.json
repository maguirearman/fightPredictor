{
  "version": 2,
  "builds": [
    {
      "src": "api/backend.py",
      "use": "@vercel/python",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/backend.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1",
      "PIP_NO_CACHE_DIR": "off",
      "PIP_DISABLE_PIP_VERSION_CHECK": "on",
      "PIP_DEFAULT_TIMEOUT": "100"
    }
  }
}
